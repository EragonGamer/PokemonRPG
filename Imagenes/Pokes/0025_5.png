.zonas-grid-gen {
    display: grid;
    grid-gap: 8px;
    grid-template-columns: 1fr 1fr 1fr;
}
.zonas-grid-gen>a{
    transition: background-size .25s, background-position .25s;
    background-size: 100%;
    background-position: 0;
	position: relative !important;
	text-shadow:
	0px 1px 1px #000,
	1px 1px 1px #000,
	1px 0px 1px #000,

	1px -1px 1px #000,
	-1px 1px 1px #000,
	0px -1px 1px #000,
	-1px -1px 1px #000,
	-1px 0px 1px #000;
	padding: 6px;
	border: 1px solid #555;
	background-color: #333;
	border-radius: 8px;
	color: #fff;
    text-decoration: none;
    font-weight: bold;
    font-size: 18px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.zonas-grid-gen>a:hover{
	background-size: 150% !important;
	background-position: -50px 0;
}
.box-gen-1{background: linear-gradient(90deg, rgba(36,142,85,1) 0%, rgba(205,25,28,1) 33%, rgba(41,124,178,1) 66%, rgba(245,201,71,1) 100%);transition: background .5s linear;}
